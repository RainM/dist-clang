executable("clang") {
  sources = [ "clang_main.cc" ]
  deps += [
    "//src/base:constants",
    "//src/base:logging",
    ":client",
  ]
}

source_set("client") {
  sources = [
    "clang.cc",
    "clang.h",
    "flag_set.cc",
    "flag_set.h",
  ]
  deps += [
    "//src/base:base",
    "//src/base:logging",
    "//src/net:net",
    "//src/proto:proto",
  ]
}
